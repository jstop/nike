<div id="header">
  <div id="logo"><a href="/"><%= image_tag 'jumpman_teal.png' %></a></div>
  <div id="title"><h2>NEXT VS NEXT: SKILLS CHALLENGE</h2></div>
</div>
<div class="clearboth"></div>
<div id="high_scores" style="height: 1500px">
</div>
<br>
<script>
  $(document).ready(function () {
    // will call refreshPartial every 4 seconds
    setInterval(refreshPartial, 3000)
  });
  // calls action refreshing the partial
  function refreshPartial() {
    $.ajax({
      url: 'controls_high_scores',
      success: function(data) {
        $('div#high_scores').html(data);
      }
    });
  }
</script>
